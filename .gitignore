.idea/
*.iws
*.iml
*.ipr
out/
!**/src/main/**/out/
!**/src/test/**/out/

# =============================================
# IDE - Eclipse
# =============================================
.apt_generated
.classpath
.factorypath
.project
.settings
.springBeans
.sts4-cache
bin/
!**/src/main/**/bin/
!**/src/test/**/bin/

# =============================================
# IDE - VS Code
# =============================================
.vscode/

# =============================================
# Kotlin
# =============================================
*.class
*.log
*.jar
!libs/*.jar

# =============================================
# Native Libraries (sensitive)
# =============================================
# Keep native libs in .gitignore if they are large
# or need to be downloaded separately
libs/*.dll
# libs/*.so
# libs/*.dylib

# =============================================
# TDLib Data (contains auth tokens and cache)
# =============================================
tdlib-data/
tdlib_data/
.tdlib/
telegram-data/

# =============================================
# Compose Desktop Build Artifacts
# =============================================
/build/compose/
/build/wix*/

